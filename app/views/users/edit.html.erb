<h1>Edit this Shnaz!</h1>

<%= form_for @user do | f | %>

<div>
        <%= f.label :username %>
        <%= f.text_field :username %>
 </div>

 <div>
        <%= f.label :email%>
        <%= f.text_field :email %>
    </div>

    <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div>
    
 <%= f.submit "Submit these changes!" %>
<% end %>

<%= link_to "Back to the list!", users_path %>