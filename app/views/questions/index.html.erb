<div class="container-fluid">
  <div class="row">
  <h1>This is the Questions index</h1>
 <h1><%= current_user.username %></h1>
  <% if logged_in? %>
  Logged in as: <%= current_user.username %> <%= link_to 'Log Out', logout_path, method: :delete %>
<% else %>
  <%= link_to 'Log In', login_path %>
<% end %>

  </div>
      <ul>
          <% @questions.each do | question | %>
              <li>
                   <strong><%= question.title %></strong>
                   <%= question.body %>
                   <%= link_to "Show Question", question_path(question)%>


                   
              </li>
          <% end %>
      </ul>
      <%= link_to "Add new question", new_question_path, class: "btn btn-info"%>
          
    


</div>